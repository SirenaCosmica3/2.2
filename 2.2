#include <stdio.h>
#include <stdlib.h>

int main()
{
    float evaluacion1, evaluacion2, prom;
    printf("\nIngrese la nota de la evaluacion 1: ");
    scanf("%f",&evaluacion1);
    printf("\nIngrese la nota de la evaluacion 2: ");
    scanf("%f",&evaluacion2);
    prom=(evaluacion1+evaluacion2)/2;
    printf("\nEl promedio es de: %2.f",prom);
    return 0;
}
